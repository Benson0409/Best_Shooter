.button {
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    height: 40px;
    width: 40px;
    background-color: rgb(255, 255, 255);
    font-size: 20px;
    align-items: auto;
    align-self: auto;
    flex-direction: row;
    position: relative;
}

.songSelect {
    width: 100px;
    height: 100px;
    max-height: none;
    min-width: auto;
    align-items: auto;
    justify-content: flex-end;
}
